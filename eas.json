{
  "cli": {
    "version": ">=11.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "android": { "buildType": "apk" },
      "distribution": "internal",
      "env": {
        "NODE_VERSION": "20.19.4",
        "YARN_IGNORE_ENGINES": "true"
      }
    },
    "production": {
      "android": { "buildType": "app-bundle" },
      "env": {
        "NODE_VERSION": "20.19.4",
        "YARN_IGNORE_ENGINES": "true"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": { "buildType": "apk" },
      "env": {
        "NODE_VERSION": "20.19.4",
        "YARN_IGNORE_ENGINES": "true"
      }
    }
  },
  "submit": { "production": {} }
}
